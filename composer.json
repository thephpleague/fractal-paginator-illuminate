{
    "name": "league/fractal-paginator-illuminate",
    "description": "Fractal paginator adapter for Illuminate",
    "keywords": [
        "league",
        "api",
        "json",
        "rest",
        "fractal",
        "pagination",
        "illuminate"
    ],
    "homepage": "http://fractal.thephpleague.com/",
    "license": "MIT",
    "require": {
        "php": ">=7.2"
    },
    "require-dev": {
        "mockery/mockery": "~1.0",
        "phpstan/phpstan": "^0.11.8",
        "phpunit/phpunit": "~8.0",
        "squizlabs/php_codesniffer": "~3.4",
        "league/fractal": "~1.0|1.0.x-dev",
        "illuminate/contracts": "~5.0"
    },
    "suggest": {
        "league/fractal": "The engine for which this adapter was built"
    },
    "autoload": {
        "psr-4": {
            "League\\Fractal\\Pagination\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "League\\Fractal\\Test\\": "test"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "conflict": {
        "league/fractal": "<1.0"
    }
}
